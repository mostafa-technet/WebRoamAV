        <KeyBinding Command="{x:Static local:Commands.btnpOKPressed}" CommandTarget="{Binding ElementName=btnExclude}" Modifiers="Alt" Key="O"/>
        <KeyBinding Command="{x:Static local:Commands.btnpCancelPressed}" CommandTarget="{Binding ElementName=btnExclude}" Modifiers="Alt" Key="C"/>
    </Window.InputBindings>
